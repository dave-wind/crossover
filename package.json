{
  "name": "crossover-ui",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "dist/index.d.ts",
  "description": "Fast build react components",
  "keywords": [
    "React",
    "Typescript",
    "UI",
    "Component"
  ],
  "homepage": "https://github.com/dave-wind/crossover",
  "repository": {
    "type":"git",
    "url":"https://github.com/dave-wind/crossover"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 6",
    "build": "npm run build-ts && npm run build-css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-a11y": "^6.0.7",
    "@storybook/addon-actions": "^6.0.7",
    "@storybook/addon-controls": "^6.0.20",
    "@storybook/addon-docs": "^6.0.7",
    "@storybook/addon-essentials": "^6.0.7",
    "@storybook/addon-knobs": "^6.0.7",
    "@storybook/addon-links": "^6.0.7",
    "@storybook/addon-storysource": "^6.0.7",
    "@storybook/addon-viewport": "^6.0.7",
    "@storybook/node-logger": "^6.0.7",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.2",
    "babel-loader": "^8.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.5",
    "styled-components": "^5.1.1"
  }
}
